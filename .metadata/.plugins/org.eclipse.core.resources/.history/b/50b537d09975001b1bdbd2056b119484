package sist;

/*
 * [문제] 아래와 같이 콘솔 창에 출력해 보세요.
 * 
 * 이름 : 이 성 욱
 * 연락처 : 010-2484-0902
 * 이메일 : sd02052@nate.com
 * 주소 : 경기도 고양시 일산동구 중산동 산들마을 2단지
 */
public class Ex03 {

	public static void main(String[] args) {
		System.out.println("이\t름: 이성욱");
		System.out.print("연락처: 010-2484-0902");
		System.out.println("\n이메일: sd02052@nate.com");
		System.out.print("주소: 경기도 고양시 일산");
		System.out.println("동구 중산동 산들마을 2단지");
	}

}
